# Prettier Config
# See: [Docs](https://prettier.io/docs/en/configuration.html)

trailingComma: "all"
tabWidth: 2
semi: true
singleQuote: true
printWidth: 160
useTabs: false
quoteProps: "consistent"
bracketSpacing: true
jsxBracketSameLine: true
arrowParens: "always"
requirePragma: false
insertPragma: false
proseWrap: "preserve"
htmlWhitespaceSensitivity: "css"
endOfLine: "lf"
embeddedLanguageFormatting: "auto"
